{
  "type": "module",
  "private": true,
  "scripts": {
    "build": "dotenv -e ./.env.production.local -v NODE_ENV=production -- nuxt build",
    "depcheck": "depcheck",
    "dev": "dotenv -e ./.env.development.local -v NODE_ENV=development -- nuxt dev",
    "generate": "dotenv -e ./.env.production.local -v NODE_ENV=production -- nuxt generate",
    "lint": "eslint --max-warnings 0 && stylelint './app/**/*.{css,scss,vue}' -i ./.gitignore --mw 0",
    "lint:fix": "eslint --fix --max-warnings 0 && stylelint './app/**/*.{css,scss,vue}' -i ./.gitignore --fix --mw 0",
    "postinstall": "nuxt prepare",
    "preview": "nuxt preview",
    "typecheck": "nuxt typecheck",
    "unused-exports": "ts-unused-exports ./tsconfig.json"
  },
  "dependencies": {
    "@kikiutils/nuxt": "^10.3.0",
    "@kikiutils/shared": "^14.0.0",
    "@nuxtjs/robots": "^5.6.7",
    "@vueuse/nuxt": "^14.1.0",
    "nuxt": "^4.2.2",
    "nuxt-security": "^2.5.0"
  },
  "devDependencies": {
    "@kikiutils/eslint-config": "^5.0.0",
    "@kikiutils/eslint-plugin-tailwindcss": "^3.19.0",
    "@kikiutils/eslint-plugin-vue": "^10.2.2",
    "@kikiutils/stylelint-config": "^0.3.1",
    "@unocss/nuxt": "^66.5.10",
    "depcheck": "^1.4.7",
    "dotenv-cli": "^11.0.0",
    "eslint-plugin-format": "^1.1.0",
    "eslint-plugin-tailwindcss": "^3.18.2",
    "nuxt-vite-plugin-webfont-dl": "^0.1.2",
    "ts-unused-exports": "^11.0.1",
    "unocss": "^66.5.10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "esbuild"
    ],
    "overrides": {
      "tailwindcss": "^3.4.18"
    }
  }
}
